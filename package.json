{
  "name": "smartfilterpro-core-ingest",
  "version": "0.1.0",
  "description": "SmartFilterPro Core Ingest Service - receives normalized runtime events, stitches sessions, and posts summaries to Bubble.io",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "migrate": "ts-node src/runMigrations.ts",
    "worker:session": "ts-node src/workers/sessionSticher.ts",
    "worker:summary": "ts-node src/workers/summaryWorker.ts --run",
    "worker:summary:hourly": "ts-node src/workers/summaryWorker.ts --hourly"
  },
  "dependencies": {
 "axios": "^1.7.2",
  "dayjs": "^1.11.11",
  "dotenv": "^16.4.5",
  "express": "^4.19.2",
  "pg": "^8.11.5",
  "zod": "^3.23.8",
  "@types/pg": "^8.10.3",
  "@types/express": "^4.17.21",
  "@types/node": "^20.14.10"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
